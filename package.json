{
	"name": "vscode-string-converter",
	"displayName": "VSCode String Converter",
	"description": "This extension converts the selected strings to a specific format",
	"author": {
		"name": "Bruno Vasconcelos",
		"email": "brunohgv2@gmail.com",
		"url": "https://github.com/brunohgv"
	},
	"publisher": "brunohgv",
	"homepage": "https://github.com/brunohgv/string-converter",
	"repository": {
		"url": "https://github.com/brunohgv/string-converter"
	},
	"version": "0.1.0",
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.camelToKebab",
		"onCommand:extension.camelToSnake",
		"onCommand:extension.camelToTitle"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.camelToKebab",
				"title": "Camel To Kebab"
			},
			{
				"command": "extension.camelToSnake",
				"title": "Camel To Snake"
			},
			{
				"command": "extension.camelToTitle",
				"title": "Camel To Title"
			}
		]
	},
	"scripts": {
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.41.0",
		"eslint": "^6.6.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2"
	}
}
